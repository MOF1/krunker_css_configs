        "hint_image": "https://cdn.discordapp.com/attachments/612260361806282793/885879817990447174/unknown.png",
        "options": [
          {
            "name": "On",
            "value": "20px",
            "raw_value": "--blur-radius2: 5px;"
          },
          {
            "name": "Off",
            "value": "0px",
            "raw_value": "--blur-radius2: 0px;"
          }
        ]
      },
